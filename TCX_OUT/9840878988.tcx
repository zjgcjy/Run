<script>
  // Detect if cookies are enabled and throw an error message if it is disabled
  if (!navigator.cookieEnabled) {
    jQuery('h1').after("<div class='error message'>Please enable browser cookies to log in.</div>");
  };
  
  // Get background class used on html element (Ref: https://strava.atlassian.net/browse/ACQN-230)
  const getBackgroundName = () => {
    const classList = document.querySelector('html') && document.querySelector('html').classList;
  
    return classList
      ? Array.from(classList).find((item) => item.includes('background-')) || null
      : null;
  }
  
  const sendLoginTrackEvent = ( action, element) => {
    Strava.ExternalAnalytics.trackV2({
      category: 'login',
      page: 'login',
      action: action,
      element: element,
      properties: {
        background: getBackgroundName()
      }
    });
  }
  
  jQuery('#facebook-signin').on('click', () => {
    sendLoginTrackEvent('click', 'facebook_login');
  });
  
  jQuery('#google-signin').on('click', () => {
    sendLoginTrackEvent('click', 'google_login');
  });
  
  jQuery('#apple-signin').on('click', () => {
    sendLoginTrackEvent('click', 'apple_login');
  });
  
  jQuery('#forgot-password').on('click', () => {
    sendLoginTrackEvent('click', 'forgot_password');
  });
  
  jQuery(document).ready(function(){
    jQuery('#login-button').on('click', function(e){
      e.preventDefault();
       var enterpriseCaptchaEnabled = false;
       if (enterpriseCaptchaEnabled) {
        grecaptcha.enterprise.ready(function() {
          grecaptcha.enterprise.execute("6Lf84MMaAAAAAMIXuJFcugYTz-KtII9sep3JVroo", {action: 'LOGIN'}).then(function(token) {
            jQuery(jQuery.find("input[name=recaptcha_enterprise_token]")).val(token)
            jQuery('#login_form').submit()
          });
        });
      } else {
        jQuery('#login_form').submit()
      }
      sendLoginTrackEvent('click', 'login');
    });
  
    jQuery('#login_form').on('submit', function(){
      jQuery('#login-button').attr('disabled', true);
    });
  
    sendLoginTrackEvent('screen_enter'); // Send track event for screen_enter
  });
</script>
